    "fastify": "^4.26.1",
    "fastify-cli": "^6.3.0",
    "fastify-plugin": "^4.0.0",
    "mongoose": "^8.6.1"
  },
  "devDependencies": {
    "@types/node": "^22.5.2",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  }
