{
  "name": "habit-tracker-api",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "happit-tracker-api/app.ts",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "build": "tsc -p .",
